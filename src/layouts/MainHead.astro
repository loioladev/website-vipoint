---
interface Props {
    title?: string;
    description?: string;
}

const { title = "Vipoint", description = "Vipoint Informática, Gráfica e Papelaria no DF." } = Astro.props as Props;
---

<head>
    <meta charset="UTF-8"/>
    <meta name="author" content="Matheus Loiola"/>
    <meta name="description" content={description}>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="icon" type="image/png" href="/icon.png"/>
    <title>{title}</title>
</head>
